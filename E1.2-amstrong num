#include<stdio.h>
int main()
{
	int n,rem,sum=0,temp;
	printf("N.Eragam reddy--192211729\n");
	printf("Enter the number: ");
	scanf("%d",&n);
	temp=n;
	while (n>0)
	{
		rem=n%10;
		sum=sum+(rem*rem*rem);
		n=n/10;
	}
	if(temp==sum)
     {
	printf("armstrong number");
   }
    else
    {
      printf("not armstrong number");
    }
     return 0;
}
